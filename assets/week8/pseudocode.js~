function factorial(val) {
    if(val == 0) {
        return 1;
    } else {
        return val * factorial(val - 1);
    }
}

function fibonacci(val) {
    if(val <= 2) {
        return val;
    } else {
        return fibonacci(val - 1) + fibonacci(val - 2);
    }
}

console.log("factorial of: %s is equal to: ", 3, factorial(3));
console.log("fibonacci of: %s is equal to: ", 3, fibonacci(3));
